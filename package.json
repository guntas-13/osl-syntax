{
  "name": "osl",
  "displayName": "osl-syntax-Highlighting",
  "description": "Syntax Highlighting for osl. (The Unambiguous Language)",
  "publisher": "GuntasSinghSaran",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.98.0"
  },
  "icon": "assets/oslMascot.png",
